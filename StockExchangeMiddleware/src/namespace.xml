<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


<bean id="address" class="com.cts.training.model.AddressEntity"
p:id="201" p:houseNumber="E-411" p:street="Street No 5" p:city="Delhi"
p:state="NewDelhi">

</bean>



<bean id="user" class="com.cts.training.model.UserEntity"
p:id="211" p:username="user123" p:password="user123#"
p:email="user@gmail.com" p:enabled="true" p:address-ref="address">

</bean>
</beans>
