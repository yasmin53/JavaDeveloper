
public class AnonymousConcrete {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Test t=new Test()
		{
			public void display()
			{
				System.out.println("good morning");
			}
		};
		t.display();
		
	}
}
		
		class Test
		{
			public void display()
			{
				System.out.println("hi");
			
			}
		}


