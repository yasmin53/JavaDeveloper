import java.util.*;
import java.util.Scanner;

public class Reverse {
	public static void main(String[] args) {
		int i,size,a[];
		Scanner s=new Scanner(System.in);
		size=s.nextInt();
		i=s.nextInt();
		a=new int[size];
		for(i=0;i<size;i++)
		{
			a[i]=s.nextInt();
			System.out.println("order is:"+a[i]);
		}
		for(i=size-1;i>=0;i--)
		{
		System.out.println("REVERSE ORDER"+a[i]);	
		}
			
			
		}
	}
