
public class ThreadCreation {

	public static void main(String[] args)
	{
		System.out.println("main thread");
		FirstThread firstThread=new FirstThread();
		firstThread.start();
		
	}
	}
	
class FirstThread extends Thread
{
public void run()
{
	for(int i=1;i<=10;i++)
	{
		System.out.println("first method:" +i);
		
	}
}
}

